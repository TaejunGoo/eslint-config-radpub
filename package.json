{
  "name": "@taejun.goo/eslint-config-radpub",
  "version": "1.0.0",
  "description": "RadPub 팀 공용 ESLint 설정 - 보안, 접근성, 코드 품질 규칙 포함",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./index.mjs",
  "exports": {
    ".": "./index.mjs",
    "./base": "./eslint/base.mjs",
    "./typescript": "./eslint/typescript.mjs",
    "./security": "./eslint/security.mjs",
    "./react-security": "./eslint/react-security.mjs",
    "./imports": "./eslint/imports.mjs",
    "./accessibility": "./eslint/accessibility.mjs",
    "./stylistic": "./eslint/stylistic.mjs",
    "./legacy": "./legacy/index.cjs",
    "./legacy/next": "./legacy/next.cjs",
    "./legacy/base": "./legacy/base.cjs",
    "./legacy/typescript": "./legacy/typescript.cjs",
    "./legacy/security": "./legacy/security.cjs",
    "./legacy/react-security": "./legacy/react-security.cjs",
    "./legacy/imports": "./legacy/imports.cjs",
    "./legacy/accessibility": "./legacy/accessibility.cjs",
    "./legacy/stylistic": "./legacy/stylistic.cjs"
  },
  "files": [
    "eslint",
    "legacy",
    "index.mjs",
    "README.md"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "nextjs",
    "typescript",
    "react",
    "radpub"
  ],
  "author": "radPub",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/eslint-config-radpub.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/eslint-config-radpub/issues"
  },
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "peerDependenciesMeta": {
    "typescript-eslint": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    }
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3"
  },
  "devDependencies": {
    "eslint": "^9.39.1"
  }
}
